<?xml version="1.0" encoding="US-ASCII"?>

<!--
    Description:
        A configuration file for testing string configurations.
-->

<config>
    
    <property name="serFile" value="/tmp/ac.ser"/>

    <component name="a" type="com.sun.labs.util.props.StringConfigurable">
        <property name="one" value="a"/>
        <property name="two" value="b"/>
        <property name="three" value="c"/>
    </component>

    <component name="ac" type="com.sun.labs.util.props.StringConfigurable" serialized="${serFile}">
        <property name="one" value="a"/>
        <property name="two" value="b"/>
        <property name="three" value="c"/>
    </component>

    <!-- Any serialized object can be here -->
    <serialized name="acs" type="com.sun.labs.util.props.StringConfigurable" location="${serFile}"/>

    <!-- Any serialized object can be here -->
    <serialized name="badClass" type="a.dangerous.group.of.Monkeys" location="${serFile}"/>

    <component name="b" type="com.sun.labs.util.props.StringConfigurable">
        <property name="one" value="d"/>
        <property name="two" value="e"/>
        <property name="three" value="f"/>
    </component>
    
    <component name="bc" type="com.sun.labs.util.props.StringConfigurable" serialized="${serFile}">
        <property name="one" value="d"/>
        <property name="two" value="e"/>
        <property name="three" value="f"/>
    </component>
</config>